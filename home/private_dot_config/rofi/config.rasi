// example: rofi -dump-config

configuration{
    modi: "combi,drun,window,pass,calc:qalc +u8 -nocurrencies";
    combi-modi: "drun,window,filebrowser,ssh";
    display-combi: "  All ";
    display-run: "  Shell ";
    display-drun: " 󰀻 Apps ";
    display-window: "  Windows ";
    display-filebrowser: "  Files ";
    display-ssh: "  SSH ";
    display-calc: "  Calc ";
    display-pass: "  Pass ";

    location: 2;
    yoffset: 100;
    disable-history: false;
    sidebar-mode: true;

    show-icons: true;
    icon-theme: "Papirus";
    drun-display-format: "{icon} {name}";

    terminal: "i3-sensible-terminal";
}

@import "theme"
